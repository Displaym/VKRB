using System;
using DBSN;
using System.IO;

namespace ExampleDBSN
{
    class Program
    {
        static void Main(string[] args)
        {
            ClDBSN dbsn = new ClDBSN("D:\\Example\\", "Gleb");
            dbsn.closeDBSN();
            dbsn.openDBSN();
            dbsn.closeDBSN();
            dbsn.writeDBSN();
            dbsn.realtime_readDBSN();
            Console.ReadLine();
        }
    }
}
